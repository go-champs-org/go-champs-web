    "build:adm": "env-cmd -f env.prod.js react-scripts --openssl-legacy-provider build",
    "build:staging": "env-cmd -f env.staging.js react-scripts --openssl-legacy-provider build",
    "test": "react-scripts test",
    "test:e2e": "cypress run",
    "test:e2e:headless": "xvfb-run -a cypress run",
